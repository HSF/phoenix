{
  "name": "phoenix-ng",
  "version": "2.14.1",
  "private": true,
  "scripts": {
    "start": "ng serve phoenix-app",
    "start:ssl": "ng serve phoenix-app --ssl --host 0.0.0.0",
    "start:single": "ng serve phoenix-app --configuration singleEvent",
    "build:prod": "ng build --configuration production",
    "build:app": "yarn build:prod phoenix-app",
    "build:ui-components": "yarn build:prod phoenix-ui-components",
    "test": "jest --no-cache",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test:coverage --maxWorkers=2 --silent",
    "e2e": "ng e2e",
    "prettier": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore",
    "prettier:check": "yarn prettier --check .",
    "prettier:write": "yarn prettier --write .",
    "eslint": "eslint projects/**/*.ts --ignore-path ../../.gitignore",
    "eslint:fix": "yarn eslint --fix",
    "deploy": "mv ../../docs/api-docs ../../api-docs && yarn build:app --output-path ../../docs --base-href \"/phoenix/\" && cp ../../docs/index.html ../../docs/404.html && mv ../../api-docs ../../docs/api-docs",
    "version": "yarn deploy",
    "deploy:web": "yarn build:app --output-path ./docs --base-href \"./\" && cp ./docs/index.html ./docs/404.html",
    "deploy:web:single": "ng build phoenix-app --configuration singleEvent --output-path ./docs --base-href \"./\" && cp ./docs/index.html ./docs/404.html"
  },
  "dependencies": {
    "@angular/animations": "^16.1.8",
    "@angular/cdk": "^16.1.7",
    "@angular/common": "^16.1.8",
    "@angular/compiler": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/forms": "^16.1.8",
    "@angular/material": "^16.1.7",
    "@angular/platform-browser": "^16.1.8",
    "@angular/platform-browser-dynamic": "^16.1.8",
    "@angular/router": "^16.1.8",
    "css-element-queries": "^1.2.3",
    "cypress-plugin-snapshots": "^1.4.4",
    "phoenix-event-display": "^2.14.1",
    "phoenix-ui-components": "^2.14.1",
    "qrcode": "1.5.3",
    "rxjs": "^7.8.1",
    "three": "^0.155.0",
    "tslib": "^2.6.1",
    "typescript": "~5.1.6",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "~16.1.8",
    "@angular/language-service": "^16.1.8",
    "@types/cypress": "^1.1.3",
    "@types/qrcode": "^1.5.1",
    "concurrently": "^8.2.0",
    "cypress": "^12.17.3",
    "jest": "^29.6.2",
    "jest-preset-angular": "^13.1.1",
    "ng-packagr": "^16.1.0"
  }
}
