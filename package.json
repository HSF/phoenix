{
  "name": "phoenix-event-display",
  "description": "Experiment independent event display",
  "author": "edward.moyse@cern.ch",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod --output-path docs --base-href \"/phoenix/\" && cp ./docs/index.html ./docs/404.html && npm run gen-api-docs",
    "gen-api-docs": "compodoc -p src/compodoc.conf.json -n \"Phoenix API Docs\" -d ./docs/api-docs && sed -i \"s/<p.*logo-text.svg.*p>//g\" ./docs/api-docs/index.html && sed -i \"s/Phoenix API Docs/<img data-src=\\\"images\\/logo-text.svg\\\" class='img-responsive' data-type=\\\"compodoc-logo\\\">/g\" ./docs/api-docs/js/menu-wc.js && cp ./src/favicon.ico ./docs/api-docs/images/favicon.ico && cp ./src/assets/images/logo-text.svg ./docs/api-docs/images/logo-text.svg",
    "test:coverage": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/phoenix/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/forms": "^10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@tweenjs/tween.js": "^17.4.0",
    "core-js": "^3.6.5",
    "coveralls": "^3.1.0",
    "css-element-queries": "^1.2.3",
    "dat.gui": "^0.7.7",
    "jszip": "^3.5.0",
    "rxjs": "^6.6.2",
    "stats-js": "^1.0.1",
    "three": "^0.120.0",
    "tslib": "^2.0.1",
    "zone.js": "^0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.6.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "^3.9.7"
  }
}
