    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@tweenjs/tween.js": "^17.4.0",
    "@types/three": "^0.103.2",
    "core-js": "^2.5.4",
    "coveralls": "^3.0.4",
    "dat.gui": "^0.7.6",
    "rxjs": "~6.3.3",
    "stats-js": "^1.0.1",
    "three": "^0.104.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
